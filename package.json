{
  "name": "your-work-search",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "compile": "gulp js css",
    "watch": "gulp watch",
    "lint": "eslint .",
    "pretest": "npm-shrinkwrap-check -3 && eslint . && npm run compile && NODE_ENV=test npm run db-migrate",
    "test-mocha": "mocha",
    "test": "NODE_ENV=test npm run test-mocha && NODE_ENV=test npm run features",
    "db-migrate": "knex migrate:latest --knexfile db/knexfile.js",
    "features": "NODE_ENV=test cucumber.js && NODE_ENV=test EXPRESS_BASE_PATH=/example-base-path cucumber.js",
    "heroku-postbuild": "npm run compile"
  },
  "// NOTE": [
    "^Pinned dependencies are actually devDependencies, which are only",
    "included so that they are available in the Heroku build pipeline"
  ],
  "dependencies": {
    "babel-preset-es2015": "^6.9.0",
    "body-parser": "~1.13",
    "bookshelf": "^0.9.5",
    "browserify": "^13.0.1",
    "consolidate": "~0.14",
    "cookie-parser": "~1.3",
    "debug": "~2.2",
    "express": "~4.13",
    "express-validator": "^2.20.5",
    "font-awesome": "^4.6.3",
    "govuk-elements-sass": "^1.1.4",
    "govuk_frontend_toolkit": "^4.10.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-plumber": "^1.1.0",
    "gulp-sass": "^2.3.1",
    "gulp-streamify": "^1.0.2",
    "gulp-uglify": "^1.5.3",
    "gulp-util": "^3.0.7",
    "hogan.js": "~3.0",
    "i18n": "^0.8.2",
    "jquery": "^2.2.4",
    "jquery-ui": "^1.10.5",
    "knex": "^0.11.5",
    "method-override": "~2.3",
    "mobile-detect": "^1.3.2",
    "moment": "^2.13.0",
    "morgan": "~1.6",
    "node-uuid": "~1.4",
    "pg": "^4.5.5",
    "serve-favicon": "~2.3",
    "vinyl-source-stream": "^1.1.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "cucumber": "^0.10.2",
    "eslint": "^2.9.0",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.8.0",
    "knex-cleaner": "^1.1.3",
    "mocha": "^2.4.5",
    "npm-shrinkwrap-check": "git+https://git@github.com/gjvis/npm-shrinkwrap-check.git#ignore-nested-package-json-files",
    "sinon": "^1.17.4",
    "zombie": "^4.2.1"
  },
  "engines": {
    "node": "6.1.0"
  }
}
