{
  "name": "your-work-search",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "clean": "rm -rf ./dist",
    "compile": "npm run clean && gulp compile",
    "watch": "gulp watch",
    "lint": "eslint .",
    "html-lint": "gulp lint-all-html",
    "pretest": "npm run compile && eslint . && NODE_ENV=test npm run db-migrate",
    "test-mocha": "mocha",
    "test": "NODE_ENV=test npm run test-mocha && NODE_ENV=test npm run features && npm run html-lint",
    "db-migrate": "knex migrate:latest --knexfile db/knexfile.js",
    "fetch-metrics": "./scripts/fetch-metrics",
    "features": "NODE_ENV=test cucumber.js && NODE_ENV=test EXPRESS_BASE_PATH=/example-base-path cucumber.js",
    "heroku-postbuild": "npm run compile",
    "snyk-protect": "snyk protect"
  },
  "// NOTE": [
    "^Pinned dependencies are actually devDependencies, which are only",
    "included so that they are available in the Heroku build pipeline"
  ],
